{
  "name": "ember-thumbor-images",
  "version": "0.5.0",
  "description": "ember-thumbor-images is a simple addon that generates safe/unsafe thumbor image urls durin build. You can use it to define your responsive images in your ember apps.",
  "keywords": [
    "ember-addon",
    "ember thumbor",
    "thumbor",
    "images",
    "responsive images"
  ],
  "bugs": {
    "url": "https://github.com/abhilashlr/ember-thumbor-images/issues"
  },
  "homepage": "https://github.com/abhilashlr/ember-thumbor-images#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhilashlr/ember-thumbor-images.git"
  },
  "license": "MIT",
  "author": "Abhilash L R",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:all",
    "test:all": "ember try:each",
    "test:ember": "ember test"
  },
  "dependencies": {
    "broccoli-funnel": "2.0.2",
    "broccoli-merge-trees": "4.2.0",
    "broccoli-plugin": "4.0.7",
    "broccoli-stew": "3.0.0",
    "ember-cli-babel": "7.23.0",
    "ember-cli-htmlbars": "5.3.1",
    "fs-extra": "9.0.1",
    "thumbor-url-builder": "0.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@glimmer/component": "1.0.3",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "1.10.1",
    "ember-cli": "~3.23.0",
    "ember-cli-code-coverage": "^1.0.0-beta.9",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "2.0.2",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.6.0",
    "ember-resolver": "8.0.2",
    "ember-source": "3.25.0",
    "ember-source-channel-url": "3.0.0",
    "ember-try": "1.4.0",
    "eslint-plugin-ember": "9.6.0",
    "eslint-plugin-node": "11.1.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "qunit-dom": "1.6.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "broccoli-asset-rev"
    ]
  }
}
